<form action="/api/courses/add" method="post">
        <h3 class="create">Create new Course</h3>
        <input type="text" id="name" name="name" placeholder="Course name">
        <input type="number" id="price" name="price" placeholder="Course price">
        <input type="text" id="img" name="img" placeholder="Course img">

        <button type="submit"><p>Create</p></button>

</form>